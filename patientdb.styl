/* header style */

header
	/*font-family: 'PT Sans', sans-serif*/
	display: flex
	align-items: center
	position: fixed
	background: white;
	top: 0
	z-index: 99999
	width: 100%
	height: 48px
	border-bottom: 1px solid #7c757f

	#header_logo
		display: inline-block
		height: 48px
		line-height: 48px
		padding: 0 20px

		h1
			font-weight: 700
			font-size: 24px
			margin: 0
			line-height: 2.1em

		a
			color: black;
			text-decoration: none

	.btn-header
		margin-left: 20px
		display: inline-block
		margin: 22px 32px

	.login-wrapper
		margin: 0 30px
		display: inline-block
		float: right


/* Main container */
#main
	top: 49px
	position: relative

/* landing */
.landing-title
	display:block
	padding: 80px 0
	text-align:center

.landing-feature
	display: block
	text-align: center

	i
		font-size: 42px

.landing-feature.disabled
	color: #ccc

/* query */
.select-container, .add-filter-container
	margin: 20px 0
	display: block

/* Button Styles */

.btn
	/*
		padding: 6px 24px
	height: 32px
		*/
	outline: none
	background: transparent
	font-weight: 700

	cursor: pointer
	transition(0.2s)

.btn-new-query
	border: 2px solid black

.btn-new-query:hover
	border: 2px solid #9c2ecc
	background: #9c3ecc
	color: #FFF

.btn-add-filter:hover
	border: 2px solid #1481CE
	background: #1481CE
	color: #FFF

.btn-submit
	border: 2px solid #2faf0f
	color: #2faf0f

.btn-submit:hover
	background: #2faf0f
	color: #FFF

.btn-get-started
	color: #1EAEDB
	border: 2px solid #1EAEDB

.btn-get-started:hover
	background: #1EAEDB
	color: #fff


/* form styles */
.new-query

	.form-content
		display:block

		.cluster-container
			margin: 20px 0

			.cluster-title
				background: #ddd
				border: 1px solid #bbb
				border-bottom: none
				width: auto
				padding: 10px 12px

			.input-cluster
				background: white
				padding: 20px
				border: 1px solid #bbb

				.input-subcluster
					border: 1px solid #ddd
					padding: 0px 20px 10px 20px
					margin: 16px 0 32px 0

					.input-category
						background: white
						position: relative
						display: inline-block
						top: -12px
						left: -6px
						padding: 0 5px

				.input-group
					display: block
					padding: 10px 0

	.form-submit
		float: left
		display: block
		background: white
		padding: 20px
		border: 1px solid #bbb
		margin: 20px 0

/* report styles */
.report-container
	width: 650px
	margin: 20px

	.report-cluster-container
		margin: 20px 0

		.report-title
			background: #ddd
			border: 1px solid #bbb
			border-bottom: none
			width: auto
			padding: 10px 12px

		.report-cluster
			background: white
			padding: 20px
			border: 1px solid #bbb

