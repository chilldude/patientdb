/* admin styles */
.table-container
	width: 90%
	overflow: scroll

/* dev styles */
.data-log
	font-family: monospace
	color: white
	position: fixed
	padding: 20px
	bottom: 0
	width: 100%
	background: rgba(0,0,0,0.8)
	height: 300px


	.hide-dev
		display: block
		text-decoration: blink
		text-align: right
		color: greenyellow
		position: relative
		margin-right: 40px

	.log-data
		overflow-y: scroll
		height: 270px
		display: block
		margin-top: 20px
